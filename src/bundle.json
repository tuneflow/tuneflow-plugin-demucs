{
    "plugins": [
      {
        "providerId": "qjq",
        "providerDisplayName": {
            "zh": "QJQ",
            "en": "QJQ"
        },
        "pluginId": "demucs",
        "pluginDisplayName": {
            "zh": "智能音频分轨",
            "en": "Smart Audio Source Separator"
        },
        "pluginDescription": {
            "zh": "从音乐中分离人声、鼓、贝斯和其余音轨",
            "en": "Separates vocal, drums, bass, and other tracks from your audio."
        },
        "version": "1.0.0",
        "supportedPlatforms": ["desktop"],
        "minRequiredDesktopVersion": "1.8.9",
        "options": {
          "allowReset": false,
          "allowManualApplyAdjust": true
        },
        "triggers": [
          {
            "type": "selected-clips",
            "config": {
              "allowedClipTypes": ["audio"],
              "maxNumClips": 1
            }
          }
        ],
        "categories": ["separate_source"],
        "icon": {
          "0.5x": "https://s.tuneflow.com/images/plugins/icons/demucs/demucs@0.5x.png",
          "1x": "https://s.tuneflow.com/images/plugins/icons/demucs/demucs@1x.png",
          "2x": "https://s.tuneflow.com/images/plugins/icons/demucs/demucs@2x.png"
        }
      }
    ]
}
